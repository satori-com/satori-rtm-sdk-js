{
  "name": "satori-rtm-sdk",
  "version": "1.2.2",
  "description": "JavaScript SDK for Satori RTM",
  "repository": {
    "type": "git",
    "url": "https://github.com/satori-com/satori-rtm-sdk-js.git"
  },
  "files": [
    "dist",
    "gulpfile.js",
    "src"
  ],
  "keywords": [
    "RTM",
    "real-time",
    "SDK"
  ],
  "main": "src/rtm.js",
  "types": "src/rtm.d.ts",
  "scripts": {
    "deploy": "gulp deploy",
    "build": "gulp build",
    "docs": "gulp docs",
    "test": "jest",
    "preversion": "npm test",
    "version": "npm run build && npm run docs",
    "postversion": "git push && git push --tags"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": ".test.js"
  },
  "author": "Satori Worldwide, Inc.",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "akamai": "0.10.1",
    "browserify": "13.0.1",
    "gulp": "3.9.1",
    "gulp-jsdoc3": "1.0.0",
    "minami": "1.2.3",
    "jest-cli": "14.1.0",
    "gulp-flatten": "0.2.0",
    "gulp-eslint": "3.0.1",
    "gulp-rename": "1.2.2",
    "gulp-util": "3.0.7",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.2",
    "rsync": "0.6.1",
    "inquirer": "0.11.0",
    "del": "1.2.0",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "12.0.0",
    "vinyl-buffer": "1.0.0",
    "run-sequence": "1.1.5",
    "vinyl-source-stream": "1.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-jsx-a11y": "2.2.2"
  },
  "dependencies": {
    "ws": "7.2.0",
    "object-assign": "4.1.0",
    "crypto-js": "3.1.6"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "browser": {
    "./src/websocket.js": "./src/websocket-browser.js"
  }
}
